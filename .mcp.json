{
  "mcpServers": {
    "lean-lsp": {
      "command": "uv",
      "args": ["run", "--directory", "/Users/eric/GitHub/lean-lsp-mcp", "lean-lsp-mcp"]
    },
    "sls-mcp": {
      "command": "uv",
      "args": ["run", "--directory", "/Users/eric/GitHub/SLS-Strange-Loop-Station/forks/sls-mcp", "sls-mcp"],
      "env": {
        "SBS_ROOT": "/Users/eric/GitHub/SLS-Strange-Loop-Station/SBS",
        "ZULIP_ENABLED": "1"
